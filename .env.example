# YouTube Data API v3 키
# https://console.cloud.google.com/apis/credentials 에서 발급받으세요
YOUTUBE_API_KEY=your_youtube_api_key_here

# Google Gemini API 키 (댓글 감정 분석용)
# https://aistudio.google.com/app/apikey 에서 발급받으세요
GEMINI_API_KEY=your_gemini_api_key_here

# 환경 설정
NODE_ENV=development

# 타임존 (선택사항)
TZ=Asia/Seoul

# 서버 포트 (선택사항, 기본값: 3000)
PORT=3000

# Docker 호스트 포트 (docker-compose용)
HOST_PORT=3000

# 호스트 (선택사항)
HOSTNAME=0.0.0.0
